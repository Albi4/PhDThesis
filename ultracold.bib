@Article{Ridinger2011lan,
  Title                    = {Large atom number dual-species magneto-optical trap for fermionic $6$Li and $^{40}$K atoms},
  Author                   = {Ridinger, A. and Chaudhuri, S. and Salez, T. and Eismann, U. and Fernandes, D. R. and Magalh{\~a}es, K. and Wilkowski, D. and Salomon, C. and Chevy, F.},
  Journal                  = {Eur. Phys. J. D},
  Year                     = {2011},
  Pages                    = {223--242},
  Volume                   = {65},

  Abstract                 = {We present the design, implementation and characterization of a dual-speciesmagneto-optical trap (MOT) for fermionic 6Li and 40K atoms withlarge atom numbers. The MOT simultaneously contains5.2 {\texttimes} 1096Li-atoms and 8.0 {\texttimes} 10940K-atoms,which are continuously loaded by a Zeeman slower for 6Li and a 2D-MOT for40K. The atom sources induce capture rates of1.2 {\texttimes} 1096Li-atoms/s and1.4 {\texttimes} 10940K-atoms/s. Trap losses due to light-induced interspeciescollisions of 65{\%} were observed and could be minimized to 10{\%} byusing low magnetic field gradients and low light powers in the repumping light of bothatomic species. The described system represents the starting point for the production of alarge-atom number quantum degenerate Fermi-Fermi mixture.},
  Day                      = {01},
